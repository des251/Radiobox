<div class="service-items">
  {{#each serviceItems}}
  <div class="service-col">
    <div class="service-item content-box">
      <div class="icon">
        <span class="{{this.icon}}"></span>
      </div>
      <div class="name">{{this.name}}</div>
      <div class="text">{{this.text}}</div>
    </div>
  </div>
  {{/each}}
</div>